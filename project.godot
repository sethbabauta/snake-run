config_version=5

[addons]

resources_spreadsheet_view/array_color_tint=100.0
resources_spreadsheet_view/color_rows=true
resources_spreadsheet_view/array_min_width=128.0
resources_spreadsheet_view/resource_preview_size=32.0
resources_spreadsheet_view/clip_headers=false
resources_spreadsheet_view/dupe_arrays=true
resources_spreadsheet_view/context_menu_on_leftclick=false

[application]

config/name="Snake Run"
run/main_scene="res://game_state/menu/menu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://sprites/icon.svg"

[autoload]

ScoreKeeper="*res://scripts/score_keeper.gd"
Utils="*res://scripts/utils.gd"
Settings="*res://config/settings.gd"
EventBus="*res://scripts/event_bus.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=640
window/size/mode=2
window/stretch/mode="viewport"
window/handheld/orientation=4

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_resource_groups/plugin.cfg", "res://addons/resources_spreadsheet_view/plugin.cfg")

